/var/log/strace.log
/var/log/syslog.encinfo
/var/log/syslog
{
	olddir /var/log/archive
	nocreate
	daily
	rotate 730
	extension .encinfo
	sharedscripts
	dateext
	dateformat -%Y%m%d%s
	missingok
	nocompress
	postrotate
		test -r /run/rsyslogd.pid && kill -HUP $(cat /run/rsyslogd.pid) &>/dev/null
	endscript
}
