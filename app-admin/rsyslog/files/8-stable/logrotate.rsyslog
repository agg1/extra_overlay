/var/log/auth.log
/var/log/cron.log
/var/log/daemon.log
/var/log/kern.log
/var/log/lpr.log
/var/log/mail.log
/var/log/news.log
/var/log/user.log
/var/log/debug.log
/var/log/messages
/var/log/iptables.log
/var/log/wxprot.log
{
	olddir /var/log/archive
	rotate 730
	daily
	missingok
	notifempty
	compress
	delaycompress
	sharedscripts
	dateext
	dateformat -%Y%m%d%s
	postrotate
		test -r /run/rsyslogd.pid && kill -HUP $(cat /run/rsyslogd.pid) &>/dev/null
	endscript
}
